{
    "extends": "accesscontrol",
    "states": [
        {
            "name": "users",
            "type": "QStringList"
        }
    ],
    "events": [
        {
            "name": "accessGranted",
            "params": [
                {
                    "name": "userId",
                    "type": "QString"
                }
            ],
            "logged": true
        }
    ],
    "actions": [
        {
            "name": "addUser",
            "params": [
                {
                    "name": "userId",
                    "type": "QString"
                }
            ],
            "logged": true
        },
        {
            "name": "removeUser",
            "params": [
                {
                    "name": "userId",
                    "type": "QString"
                }

            ],
            "logged": true
        }
    ]
}
